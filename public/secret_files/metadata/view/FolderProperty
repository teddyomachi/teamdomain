                                            "\r",
                                            "\tformY.method = 'POST';\r",
                                            "\tformY.action = request_redirect_uri;\r",
                                            "\r",
                                            "\t//FormDataに対応していないブラウザの場合\r",
                                            "\tif(!window.FormData){\r",
                                            "\t\tform.submit();\r",
                                            "\t\treturn;\r",
                                            "\t} else {\r",
                                            "\t\t// FormData オブジェクトを作成する（引数に送信したいデータを持つフォームエレメントを指定）\r",
                                            "\t\tvar form_data = new FormData(formY);\r",
                                            "\t\t// XMLHttpRequest オブジェクトを作成\r",
                                            "\t\tvar xhr = XMLHttpRequestCreate();\r",
                                            "\t\t// POST メソッドで接続先 URL を指定（フォームの method 属性と action 属性の値を使用）\r",
                                            "\t\txhr.open(form.method, form.action);\r",
                                            "\t\t// 送信を開始する（送信したいデータを引数にセット）\r",
                                            "\t\txhr.send(form_data);\r",
                                            "\t}\r",
                                            "}\r",
                                            "\r",
                                            "// ------------------------------------------------------------\r",
                                            "// XMLHttpRequest オブジェクトを作成する関数\r",
                                            "// ------------------------------------------------------------\r",
                                            "function XMLHttpRequestCreate(){\r",
                                            "\ttry {\r",
                                            "\t\treturn new XMLHttpRequest();\r",
                                            "\t} catch(e) {}\r",
                                            "\t\ttry {\r",
                                            "\t\t\treturn new ActiveXObject('MSXML2.XMLHTTP.6.0');\r",
                                            "\t\t} catch(e) {}\r",
                                            "\t\t\ttry {\r",
                                            "\t\t\t\treturn new ActiveXObject('MSXML2.XMLHTTP.3.0');\r",
                                            "\t\t\t} catch(e) {}\r",
                                            "\t\t\t\ttry {\r",
                                            "\t\t\t\t\treturn new ActiveXObject('MSXML2.XMLHTTP');\r",
                                            "\t\t\t\t} catch(e) {}\r",
                                            "\r",
                                            "\t\t\t\t\treturn null;\r",
                                            "\t\t\t\t}\r",
                                            ""
                                        ]
