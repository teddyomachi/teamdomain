                                        {
                                            "type": "basicfunction",
                                            "reference": {
                                                "name": "items",
                                                "type": "array"
                                            },
                                            "codeClass": null,
                                            "userConfig": {
                                                "fn": "prepareData",
                                                "designer|params": [
                                                    "data"
                                                ],
                                                "implHandler": [
                                                    "var lock_check = \"\";\r",
                                                    "if (data.lock === 0) { lock_check = \"small_icon/no_locked.png\";}\r",
                                                    "if (data.lock === 1) { lock_check = \"small_icon/my_locked.png\";}\r",
                                                    "if (data.lock === 2) { lock_check = \"small_icon/locked.png\";}\r",
                                                    "if (data.lock === 4) { lock_check = \"small_icon/sco.png\";}\r",
                                                    "if (data.lock === 8) { lock_check = \"small_icon/eco.png\";}\r",
                                                    "\r",
                                                    "var fileSizeUpper = data.file_size_upper;\r",
                                                    "var fileSize      = data.file_size;\r",
                                                    "var size          = fileSizeUpper * Math.pow(2, 31) + fileSize;\r",
                                                    "var rounded_size;\r",
                                                    "\r",
                                                    "if (size === \"\" || size === 0) {\r",
                                                    "\trounded_size = \"-\";\r",
                                                    "} else if (size < 1024) {\r",
                                                    "\trounded_size = size + \" B\";\r",
                                                    "} else if (size < 1048576) {\r",
                                                    "\trounded_size = (Math.round(size / 1024)) + \" KB\";\r",
                                                    "} else if (size < 1073741824) {\r",
                                                    "\trounded_size = (Math.round(((size*10) / 1048576))/10) + \" MB\";\r",
                                                    "} else if (size < 1099511627776) {\r",
                                                    "\trounded_size = (Math.round(((size*100) / 1073741824))/100) + \" GB\";\r",
                                                    "} else if (size < 1125899906842624) {\r",
                                                    "\trounded_size = (Math.round(((size*100) / 1099511627776))/100) + \" TB\";\r",
                                                    "} else {\r",
                                                    "\trounded_size = (Math.round(((size*100) / 1125899906842624))/100) + \" PB\";\r",
                                                    "}\r",
                                                    "\r",
                                                    "Ext.apply(data, {\r",
                                                    "\tshortName: Ext.util.Format.ellipsis(data.file_name, 20),\r",
                                                    "\tlock_check_img: lock_check,\r",
                                                    "\tfile_disp_size: rounded_size\r",
                                                    "});\r",
                                                    "\r",
                                                    "return data;"
                                                ]
                                            },
                                            "designerId": "c6358269-4b4f-4ee3-a3c7-317b944eea30"
                                        },
                                        {
