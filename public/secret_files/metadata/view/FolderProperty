                                            "\t\t\t}\r",
                                            "\t\t},\r",
                                            "\r",
                                            "\t\tfailure: function(response){\r",
                                            "\t\t\tif (action.failureType === CONNECT_FAILURE) {\r",
                                            "\t\t\t\tExt.Msg.alert('Error', 'Status:'+action.response.status+': '+ action.response.statusText);\r",
                                            "\t\t\t} else if (action.failureType === SERVER_INVALID){\r",
                                            "\t\t\t\tExt.Msg.alert('Invalid', action.result.errormsg);\r",
                                            "\t\t\t} else {\r",
                                            "\t\t\t\tExt.Msg.show({\r",
                                            "\t\t\t\t\ttitle:'ファイルの追加失敗',\r",
                                            "\t\t\t\t\tmsg: up_filename +'<br/> のアップロードに失敗しました。',\r",
                                            "\t\t\t\t\tbuttons: Ext.Msg.OK\r",
                                            "\t\t\t\t});\r",
                                            "\t\t\t}\r",
                                            "\t\t}\r",
                                            "\t});\r",
                                            "}\r",
                                            "\r",
                                            "//ファイルリスト再読み込み用\r",
                                            "function requestRefresh() {\r",
                                            "\t//console.log(formX.session_id);\r",
                                            "\t//console.log(formX.cont_location);\r",
                                            "\t//console.log(formX.hash_key);\r",
                                            "\r",
                                            "\tsending_session_id\t\t= formX.session_id;\r",
                                            "\tsending_cont_location\t= formX.cont_location;\r",
                                            "\tsending_hash_key\t\t= formX.hash_key;\r",
                                            "\r",
                                            "\tvar requestRefreshData\t= Ext.apply({session_id: sending_session_id}, {cont_location: sending_cont_location});\r",
                                            "\trequestRefreshData\t= Ext.apply(requestRefreshData, {hash_key: sending_hash_key});\r",
                                            "\trequestRefreshData\t= Ext.apply({event_type: \"property_upload_file\"}, requestRefreshData);\r",
                                            "\trequestRefreshData\t= Ext.apply({request_type: \"update_file_list\"}, requestRefreshData);\r",
                                            "\t//console.log(requestRefreshData);\r",
                                            "\r",
                                            "\tExt.Ajax.request({\r",
                                            "\t\turl: 'tdx/updatedata.tdx',\r",
                                            "\t\tjsonData: requestRefreshData,\r",
                                            "\t\tsuccess: handleSuccess2,\r",
                                            "\t\tfailure: handleFailure2\r",
                                            "\t});\r",
                                            "\r",
                                            "}\r",
                                            "\r",
                                            "function handleSuccess2(response) {\r",
                                            "\tobj = Ext.decode(response.responseText);\r",
                                            "\r",
                                            "\tvar request_success = obj.success;\r",
                                            "\tvar request_status  = obj.status;\r",
                                            "\r",
                                            "\tif (request_success === false) {\r",
                                            "\t\tvar request_errors  = obj.errors;\r",
                                            "\t\tExt.Msg.show({\r",
                                            "\t\t\ttitle:'ファイル送信失敗',\r",
                                            "\t\t\tmsg: request_errors,\r",
                                            "\t\t\tbuttons: Ext.Msg.OK\r",
                                            "\t\t});\r",
                                            "\t} else {\r",
                                            "\t\tif (formX.cont_location === 'folder_a') {\r",
                                            "\t\t\tExt.getStore('FileDataStoreA').load();\r",
                                            "\t\t\tExt.getStore('FileDataStoreB').load();\r",
                                            "\t\t} else if (formX.cont_location === 'folder_b') {\r",
                                            "\t\t\tExt.getStore('FileDataStoreA').load();\r",
                                            "\t\t\tExt.getStore('FileDataStoreB').load();\r",
                                            "\t\t}\r",
                                            "\t}\r",
                                            "}\r",
                                            "\r",
                                            "function handleFailure2(response) {\r",
                                            "\tExt.Msg.show({\r",
                                            "\t\ttitle:'ファイル追加失敗',\r",
                                            "\t\tmsg: 'サーバとの通信に失敗しました。',\r",
                                            "\t\tbuttons: Ext.Msg.OK\r",
                                            "\t});\r",
                                            "}\r",
