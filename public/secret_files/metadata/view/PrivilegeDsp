                                                                    "\t\t} else if (thisForm.cont_location === 'folder_b') {\r",
                                                                    "\t\t\tExt.getStore('FileDataStoreB').load();\r",
                                                                    "\t\t}\r",
                                                                    "\t\tExt.getCmp('notification_file_bar').moveFirst();\r",
                                                                    "\t}\r",
                                                                    "\treturn;\r",
                                                                    "}\r",
