                                            "\t\ttitle:'追加ファイル選択',\r",
                                            "\t\tmsg: '追加ファイルを選択して下さい。',\r",
                                            "\t\ticon: Ext.Msg.ERROR,\r",
                                            "\t\tbuttons: Ext.Msg.OK\r",
                                            "\t});\r",
                                            "\treturn;\r",
                                            "} else {\r",
                                            "\tvar dataY = Ext.apply({session_id: this_session_id}, formX);\r",
                                            "\tdataY = Ext.apply({upload_filename: up_filename}, dataY);\r",
                                            "\tdataY = Ext.apply({request_type: \"upload_root_file\"}, dataY);\r",
                                            "\r",
                                            "\tExt.Ajax.request({\r",
                                            "\t\turl: 'tdx/updatedata.tdx',\r",
                                            "\t\tjsonData: dataY,\r",
                                            "\t\tsuccess: handleSuccess,\r",
                                            "\t\tfailure: handleFailure\r",
                                            "\t});\r",
                                            "}\r",
                                            "\r",
                                            "function handleSuccess(response) {\r",
                                            "\tobj = Ext.decode(response.responseText);\r",
                                            "\r",
                                            "\tvar request_success      = obj.success;\r",
                                            "\tvar request_hashkey      = obj.hashkey;\r",
                                            "\tvar request_redirect_uri = obj.redirect_uri;\r",
                                            "\tvar new_filename         = this_session_id + request_hashkey;\r",
                                            "\r",
                                            "\tif (request_success === false) {\r",
                                            "\t\tvar request_errors = obj.errors;\r",
                                            "\t\tExt.Msg.show({\r",
                                            "\t\t\ttitle:'ファイル送信失敗',\r",
                                            "\t\t\tmsg: request_errors,\r",
                                            "\t\t\tbuttons: Ext.Msg.OK\r",
                                            "\t\t});\r",
                                            "\t} else {\r",
                                            "\t\tExt.getCmp('createRootFile').getForm().setValues({\r",
                                            "\t\t\tanother_name: new_filename\r",
                                            "\t\t});\r",
                                            "\t\tsendFile(form, up_filename, request_redirect_uri);\r",
                                            "\t}\r",
                                            "\treturn;\r",
                                            "}\r",
                                            "\r",
                                            "function handleFailure(response) {\r",
                                            "\tExt.Msg.show({\r",
                                            "\t\ttitle:'ファイル追加失敗',\r",
                                            "\t\tmsg: 'サーバとの通信に失敗しました。',\r",
                                            "\t\tbuttons: Ext.Msg.OK\r",
                                            "\t});\r",
                                            "\treturn;\r",
                                            "}\r",
                                            "\r",
                                            "\r",
                                            "function sendFile(form, up_filename, request_redirect_uri) {\r",
                                            "\tform.submit({\r",
                                            "\t\turl: request_redirect_uri,\r",
                                            "\t\twaitMsg: 'ファイルを追加中です...',\r",
                                            "\r",
                                            "\t\tsuccess: function(form, action){\r",
                                            "\t\t\tif(action.result.success === true){\r",
                                            "\t\t\t\tExt.Msg.show({\r",
                                            "\t\t\t\t\ttitle:'ファイルの追加完了',\r",
                                            "\t\t\t\t\tmsg: up_filename +'<br/> をサーバに追加しました。',\r",
                                            "\t\t\t\t\tbuttons: Ext.Msg.OK\r",
                                            "\t\t\t\t});\r",
                                            "\t\t\t\tif (formX.cont_location === 'folder_a') {\r",
                                            "\t\t\t\t\tExt.getStore('FileDataStoreA').load();\r",
                                            "\t\t\t\t} else if (formX.cont_location === 'folder_b') {\r",
                                            "\t\t\t\t\tExt.getStore('FileDataStoreB').load();\r",
                                            "\t\t\t\t}\r",
                                            "\t\t\t\tform._fields.items[5].value = '';\r",
                                            "\t\t\t}\r",
                                            "\r",
                                            "\t\t},\r",
                                            "\r",
                                            "\t\tfailure: function(form, action){\r",
                                            "\t\t\tif (action.failureType === CONNECT_FAILURE) {\r",
                                            "\t\t\t\tExt.Msg.alert('Error', 'Status:'+action.response.status+': '+ action.response.statusText);\r",
                                            "\t\t\t}\r",
                                            "\t\t\tif (action.failureType === SERVER_INVALID){\r",
                                            "\t\t\t\tExt.Msg.alert('Invalid', action.result.errormsg);\r",
                                            "\t\t\t}\r",
                                            "\t\t}\r",
                                            "\t});\r",
                                            "}\r",
                                            ""
                                        ]
