                                            "\tif (request_success === false) {\r",
                                            "\t\tvar request_errors = obj.errors;\r",
                                            "\t\tExt.Msg.show({\r",
                                            "\t\t\ttitle:'サブフォルダの作成失敗',\r",
                                            "\t\t\tmsg: request_errors,\r",
                                            "\t\t\tbuttons: Ext.Msg.OK\r",
                                            "\t\t});\r",
                                            "\t} else {\r",
                                            "\t\trequestRefresh();\r",
                                            "\t}\r",
                                            "\treturn;\r",
                                            "}\r",
                                            "\r",
                                            "function handleFailure(response) {\r",
                                            "\tExt.Msg.show({\r",
                                            "\t\ttitle:'サブフォルダの作成失敗',\r",
                                            "\t\tmsg: 'サーバとの通信に失敗しました。',\r",
                                            "\t\tbuttons: Ext.Msg.OK\r",
                                            "\t});\r",
                                            "}\r",
                                            "\r",
                                            "//ファイルリスト再読み込み用\r",
                                            "function requestRefresh() {\r",
                                            "\tsending_cont_location\t= formX.cont_location;\r",
                                            "\tsending_hash_key\t\t= formX.hash_key;\r",
                                            "\r",
                                            "\tvar requestRefreshData\t= Ext.apply({session_id: this_session_id}, {cont_location: sending_cont_location});\r",
                                            "\trequestRefreshData\t= Ext.apply(requestRefreshData, {hash_key: sending_hash_key});\r",
                                            "\trequestRefreshData\t= Ext.apply({event_type: \"property_create_subfolder\"}, requestRefreshData);\r",
                                            "\trequestRefreshData\t= Ext.apply({request_type: \"update_folder_list\"}, requestRefreshData);\r",
                                            "\r",
                                            "\tExt.Ajax.request({\r",
                                            "\t\turl: 'tdx/updatedata.tdx',\r",
                                            "\t\tjsonData: requestRefreshData,\r",
                                            "\t\tsuccess: handleSuccess2,\r",
                                            "\t\tfailure: handleFailure2\r",
                                            "\t});\r",
                                            "\r",
                                            "}\r",
                                            "\r",
                                            "function handleSuccess2(response) {\r",
                                            "\tobj = Ext.decode(response.responseText);\r",
                                            "\r",
                                            "\tvar request_success = obj.success;\r",
                                            "\tvar request_status  = obj.status;\r",
                                            "\r",
                                            "\tif (request_success === false) {\r",
                                            "\t\tvar request_errors  = obj.errors;\r",
                                            "\t\tExt.Msg.show({\r",
                                            "\t\t\ttitle:'サブフォルダの作成失敗',\r",
                                            "\t\t\tmsg: request_errors,\r",
                                            "\t\t\tbuttons: Ext.Msg.OK\r",
                                            "\t\t});\r",
                                            "\t} else {\r",
                                            "\t\t//新規にフォルダを作成した場合に、表示に関連するのは該当Locationだけではないので、以下3カ所をコメントアウトし、条件分岐をなくした。\r",
                                            "\t\t//厳密には、選択しているワークエリアやツリーの表示状態に依存するが、処理が大変なので、一括リロードとする。\r",
                                            "\t\tvar thisDomain\t\t= Ext.ComponentQuery.query('#activeData')[0].items.items[1].value;\r",
                                            "\t\tvar anotherDomain\t= Ext.ComponentQuery.query('#activeData')[0].items.items[3].value;\r",
                                            "\t\t//console.log(thisDomain);\r",
                                            "\t\t//console.log(anotherDomain);\r",
                                            "\r",
                                            "\r",
                                            "\t\tif (thisDomain === anotherDomain) {\r",
                                            "\t\t\t//選択しているワークエリアが同一ならば、以下を実行する。\r",
                                            "\t\t\tif (formX.cont_location === 'folder_a') {\r",
                                            "\t\t\t\t//もし、作業対象のフォルダツリーがAならば、先にBを読み、その後にコールバックでAを読む。\r",
                                            "\t\t\t\t//この処理によって、プロパティペイン側は、Aを表示することになる。\r",
                                            "\t\t\t\tExt.getStore('FolderDataStoreB').load({\r",
                                            "\t\t\t\t\tcallback: function() {\r",
                                            "\t\t\t\t\t\tExt.getStore('FolderDataStoreA').load();\r",
                                            "\t\t\t\t\t}\r",
                                            "\t\t\t\t});\r",
                                            "\r",
                                            "\t\t\t\tExt.getStore('TargetFolderAT').load();\r",
                                            "\t\t\t\tExt.getStore('TargetFolderATFi').load();\r",
                                            "\r",
                                            "\t\t\t\tExt.getStore('TargetFolderBT').load();\r",
                                            "\t\t\t\tExt.getStore('TargetFolderBTFi').load();\r",
                                            "\r",
                                            "\t\t\t\tExt.getStore('FileDataStoreA').load();\r",
                                            "\t\t\t\tExt.getStore('FileDataStoreB').load();\r",
                                            "\r",
                                            "\t\t\t} else if (formX.cont_location === 'folder_b') {\r",
                                            "\t\t\t\t//もし、作業対象のフォルダツリーがBならば、先にAを読み、その後にコールバックでBを読む。\r",
                                            "\t\t\t\t//この処理によって、プロパティペイン側は、Bを表示することになる。\r",
                                            "\t\t\t\tExt.getStore('FolderDataStoreA').load({\r",
                                            "\t\t\t\t\tcallback: function() {\r",
                                            "\t\t\t\t\t\tExt.getStore('FolderDataStoreB').load();\r",
                                            "\t\t\t\t\t}\r",
                                            "\t\t\t\t});\r",
                                            "\r",
                                            "\t\t\t\tExt.getStore('TargetFolderAT').load();\r",
                                            "\t\t\t\tExt.getStore('TargetFolderATFi').load();\r",
                                            "\r",
                                            "\t\t\t\tExt.getStore('TargetFolderBT').load();\r",
                                            "\t\t\t\tExt.getStore('TargetFolderBTFi').load();\r",
                                            "\r",
                                            "\t\t\t\tExt.getStore('FileDataStoreA').load();\r",
                                            "\t\t\t\tExt.getStore('FileDataStoreB').load();\r",
                                            "\t\t\t}\r",
                                            "\t\t} else {\r",
                                            "\t\t\t//選択しているドメインが同一でないならば、以下を実行する。\r",
                                            "\t\t\tif (formX.cont_location === 'folder_a') {\r",
                                            "\t\t\t\tExt.getStore('FolderDataStoreA').load();\r",
                                            "\t\t\t\tExt.getStore('TargetFolderAT').load();\r",
                                            "\t\t\t\tExt.getStore('TargetFolderATFi').load();\r",
                                            "\t\t\t\tExt.getStore('FileDataStoreA').load();\r",
                                            "\t\t\t} else {\r",
                                            "\t\t\t\tExt.getStore('FolderDataStoreB').load();\r",
                                            "\t\t\t\tExt.getStore('TargetFolderBT').load();\r",
                                            "\t\t\t\tExt.getStore('TargetFolderBTFi').load();\r",
                                            "\t\t\t\tExt.getStore('FileDataStoreB').load();\r",
                                            "\t\t\t}\r",
                                            "\t\t}\r",
                                            "\t}\r",
                                            "}\r",
                                            "\r",
                                            "function handleFailure2(response) {\r",
                                            "\tExt.Msg.show({\r",
                                            "\t\ttitle:'サブフォルダの作成失敗',\r",
                                            "\t\tmsg: 'サーバとの通信に失敗しました。',\r",
                                            "\t\tbuttons: Ext.Msg.OK\r",
                                            "\t});\r",
                                            "}"
                                        ]
                                    },
                                    "designerId": "f6222356-7170-4184-acdf-d879be8a0ea8"
                                }
                            ]
                        }
